              <script>

 function wallpaper() {
                             var x = document.getElementById("mystery");
			   if (x.style.display === "block") {
                            x.style.display = "none";

			   }
			   else 
			   {
				   x.style.display = "block";
			   }
			   
 
		   }
  function changeWallpaper(){
         var wallpaper = document.getElementById('mystery').selectedIndex;
         var changev = document.getElementsByTagName("option")[wallpaper].value;
	 if(changev === "wall1"){
		document.getElementById("phone").style = "background-image: url('https://wallpaperaccess.com/full/833312.png');";


	}
	else if(changev === "wall2"){
		document.getElementById("phone").style = "background-image: url('https://i.pinimg.com/originals/2d/44/e9/2d44e965dff94b7aa7a51fb42f25faf8.gif');";
	}
	else if(changev === "wall3"){
           	document.getElementById("phone").style = "background-image: url('https://i.pinimg.com/originals/e8/68/5a/e8685a9cd5689d70211afc6b6f74f756.gif');";
	}
        else{
          alert("Invalid Options;");
	}

    }
	
</script>
